#include <stdio.h>
#include <string.h>

int main()
{
	int n;
	int arr1[16];
	int arr2[16];
	int arr3[16];
	char map[17][17] = { "\0" ,};
	char a[2] = "#";
	char b[2] = " ";

	int num = 1;
	int num_tmp;
	int binary;

	scanf("%d", &n);
	int i, j;
	for (i = 0; i < n; i++)
	{
		scanf("%d", &arr1[i]);
	}
	for (i = 0; i < n; i++)
	{
		scanf("%d", &arr2[i]);
	}

	for (i = 0; i < n; i++)
	{
		num = num * 2;
		arr3[i] = arr1[i] | arr2[i];
	}
	num = num / 2;
	num_tmp = num;
	// #과 공백으로된 지도 저장
	for (i = 0; i < n; i++)
	{
		num = num_tmp;
		for (j = 0; j < n; j++)
		{
			binary = arr3[i] / num;
			if (binary == 1)
			{
				strcat(map[i], a);
			}
			else
			{
				strcat(map[i], b);
			}
			arr3[i] = arr3[i] % num;
			num = num / 2;
		}
	}
	printf("[");
	for (i = 0; i < n; i++)
	{
		if (i == n-1)
		{
			printf("\"%s\"", map[i]);
			break;
		}
		printf("\"%s\", ", map[i]);
	}
	printf("]");
	printf("\n");

	return 0;
}
